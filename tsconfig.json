{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "lib": [
      "esnext"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "types": [
      "node"
    ],
    "target": "esnext"
  },
  "exclude": [
    "node_modules"
  ]
}
